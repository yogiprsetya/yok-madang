{
  "env": {
    "browser": true,
    "es2021": true,
    "node": true
  },
  "extends": ["airbnb-base"],
  "parserOptions": {
    "ecmaVersion": 12,
    "sourceType": "module"
  },
  "rules": {
    "no-console": 0,
    "indent": ["error", 2],
    "linebreak-style": 0,
    "object-curly-newline": "off",
    "no-underscore-dangle": "off",
    "arrow-parens": [2, "as-needed"]
  }
}
